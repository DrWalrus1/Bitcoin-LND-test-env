alias btctestd="bitcoind -regtest -datadir=$PWD/bitcoin"
alias btctest-cli="bitcoin-cli -regtest -datadir=$PWD/bitcoin"

alias lnd1="lnd --lnddir=$PWD/lnd1/"
alias lncli1="lncli -n regtest --lnddir=$PWD/lnd1/"

alias lnd2="lnd --lnddir=$PWD/lnd2/"
alias lncli2="lncli -n regtest --lnddir=$PWD/lnd2/"
